<h2>メンバー登録</h2>
<%= form_with model: @member do |f| %>
  <div>
    <%= f.label :image, "プロフィール写真" %>
    <%= f.file_field :image %>
  </div>
  <div>
    <%= f.label :name, "氏名" %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :password, "パスワード" %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation, "パスワード確認" %>
    <%= f.password_field :password_confirmation %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>